{
  "name": "data-structure-visualizer",
  "version": "1.0.0",
  "description": "An interactive and responsive tool for visualizing data structures and algorithms",
  "main": "index.js",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:unit": "jest --testPathPattern=\\.test\\.(ts|tsx)$",
    "test:integration": "jest --testPathPattern=integration",
    "test:components": "jest --testPathPattern=components.*\\.test\\.(ts|tsx)$",
    "test:utils": "jest --testPathPattern=utils.*\\.test\\.(ts|tsx)$",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:performance": "jest --testPathPattern=performance",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:all": "jest --coverage --verbose --passWithNoTests",
    "audit:a11y": "pa11y http://localhost:3000",
    "audit:lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "test:clear-cache": "jest --clearCache",
    "eject": "react-scripts eject",
    "build:analyze": "set ANALYZE=true && npm run build",
    "build:stats": "npm run build -- --stats",
    "build:production": "set NODE_ENV=production && npm run build",
    "analyze": "node scripts/analyze-bundle.js",
    "build:analyze:report": "npm run build && npm run analyze",
    "serve": "serve -s build -l 3000",
    "serve:analyze": "npm run build:analyze:report && npm run serve",
    "size-limit": "npm run build && node scripts/analyze-bundle.js",
    "clean": "rimraf build dist coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .tsx,.ts --max-warnings 0",
    "lint:fix": "eslint src --ext .tsx,.ts --fix",
    "fix-deps": "node scripts/fix-dependencies.js"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "framer-motion": "^10.12.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.14.1",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "styled-components": "^6.0.4"
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "prismjs": "^1.30.0",
    "webpack-dev-server": "^4.15.1",
    "svgo": "^3.0.0",
    "eslint": "^9.15.0",
    "w3c-hr-time": "^1.0.2",
    "sourcemap-codec": "@jridgewell/sourcemap-codec@^1.4.15",
    "rollup-plugin-terser": "@rollup/plugin-terser@^0.4.4",
    "puppeteer": "^24.9.0",
    "inflight": "^1.0.6",
    "glob": "^10.3.10",
    "domexception": "^4.0.0",
    "abab": "^2.0.6",
    "@humanwhocodes/object-schema": "@eslint/object-schema@^2.1.4",
    "@humanwhocodes/config-array": "@eslint/config-array@^0.18.0",
    "rimraf": "^6.0.1",
    "source-map": "^0.7.4",
    "@babel/plugin-proposal-private-methods": "@babel/plugin-transform-private-methods@^7.25.9",
    "@babel/plugin-proposal-optional-chaining": "@babel/plugin-transform-optional-chaining@^7.25.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "@babel/plugin-transform-nullish-coalescing-operator@^7.25.9",
    "@babel/plugin-proposal-class-properties": "@babel/plugin-transform-class-properties@^7.25.9",
    "@babel/plugin-proposal-numeric-separator": "@babel/plugin-transform-numeric-separator@^7.25.9",
    "@babel/plugin-proposal-private-property-in-object": "@babel/plugin-transform-private-property-in-object@^7.25.9",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-background-sync": "^7.1.0",
    "workbox-google-analytics": "^7.1.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@playwright/test": "^1.49.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/jest-axe": "^3.5.9",
    "@axe-core/playwright": "^4.10.0",
    "babel-jest": "^29.7.0",
    "compression-webpack-plugin": "^11.1.0",
    "@eslint/js": "^9.15.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "gzip-size": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-styled-components": "^7.2.0",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^3.0.1",
    "lighthouse": "^12.2.1",
    "pa11y": "^8.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
